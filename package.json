{
  "name": "fintrack-platform-v5",
  "version": "5.0.3",
  "description": "Next-generation personal finance platform with intelligent benchmarking",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "db:setup": "./scripts/setup-dev-db.sh",
    "db:setup-local": "./scripts/setup-local-postgres.sh",
    "pre-deploy": "./scripts/pre-deploy.sh",
    "ci-check": "npm run lint && npm run type-check && npm run test && npm run build",
    "release": "./scripts/generate-release.sh",
    "generate-qa-plan": "node scripts/generate-qa-plan.js",
    "validate-test-cases": "node scripts/validate-test-cases.js",
    "create-feature": "./scripts/create-feature.sh",
    "deploy": "./scripts/deploy-to-vercel.sh",
    "deploy:staging": "./scripts/deploy-with-migrations.sh staging",
    "deploy:production": "./scripts/deploy-with-migrations.sh production",
    "deploy:check": "npm run build && echo 'âœ… Ready for deployment'",
    "db:start": "docker compose up -d postgres redis",
    "db:stop": "docker compose stop postgres redis",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:backup": "./scripts/backup-local-db.sh",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "seed:generate": "tsx scripts/intelligent-seed-generator.ts",
    "cube:populate": "npx tsx scripts/populate-cube.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:auth": "tsx scripts/test-auth.ts",
    "generate-release": "node scripts/generate-release-docs.js $(node -p \"require('./package.json').version\")",
    "pre-release": "npm run build && npm run type-check && npm audit --audit-level=moderate",
    "backup-data": "node scripts/backup-data.js",
    "validate-data": "node scripts/validate-data-integrity.js",
    "deploy-check": "npm run ci-check",
    "deploy:preview": "./scripts/deploy-to-vercel.sh --preview",
    "deploy:force": "./scripts/deploy-to-vercel.sh --force",
    "migrate:safe": "./scripts/safe-migrate.sh",
    "migrate:staging": "./scripts/safe-migrate.sh staging",
    "migrate:production": "./scripts/safe-migrate.sh production",
    "rollback": "vercel rollback --previous",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^15.0.0",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.17.0",
    "@tanstack/react-query-devtools": "^5.17.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.0",
    "framer-motion": "^10.16.0",
    "ioredis": "^5.3.2",
    "jose": "^5.1.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.300.0",
    "next": "^15.0.0",
    "next-auth": "^4.24.5",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "recharts": "^2.15.4",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-next": "^15.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.4.2",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "fintech",
    "personal-finance",
    "budgeting",
    "expense-tracking",
    "financial-analytics",
    "benchmarking",
    "nextjs",
    "typescript",
    "prisma",
    "postgresql"
  ],
